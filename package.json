{
  "name": "my-only-either",
  "version": "1.2.0",
  "description": "Only support either interface",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/my-only-either.git"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "dts-bundle": "dts-bundle-generator dist/src/index.d.ts -o dist/index.d.ts --no-banner",
    "postdts-bundle": "rimraf dist/src",
    "prewebpack:prod": "npm run clean",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "postwebpack:prod": "npm run dts-bundle",
    "prewebpack:dev": "npm run clean",
    "webpack:dev": "cross-env NODE_ENV=production webpack --config webpack.config.dev.js",
    "postwebpack:dev": "npm run dts-bundle",
    "prepub": "npm run webpack:dev",
    "pub": "npm publish --registry http://localhost:8901 --force",
    "prepub:prod": "npm run webpack:prod",
    "pub:prod": "npm publish",
    "lint": "eslint --ext ts,tsx ./src/*"
  },
  "keywords": [
    "either",
    "typescript"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "author": "ByungJoon Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/my-only-either/issues"
  },
  "homepage": "https://github.com/imjuni/my-only-either#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "cross-env": "^7.0.3",
    "dts-bundle-generator": "^6.12.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^14.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.2"
  }
}